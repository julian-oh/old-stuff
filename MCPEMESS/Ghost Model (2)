function addghostmodelRenderType(renderer) { var model = renderer.getModel();var body = model.getPart('body').clear();var head = model.getPart('head').clear(); var rightArm = model.getPart('rightArm').clear(); var leftArm = model.getPart('leftArm').clear(); var rightLeg = model.getPart('rightLeg').clear(); var leftLeg = model.getPart('leftLeg').clear(); body.setTextureSize(64,64); head.setTextureSize(64,64); rightArm.setTextureSize(64,64); leftArm.setTextureSize(64,64); rightLeg.setTextureSize(64,64); leftLeg.setTextureSize(64,64);body.setTextureOffset(0,0); body.addBox(-4,-8,-2,8,8,8); body.setTextureOffset(0,16, true); body.addBox(-4,0,0,8,19,4); body.setTextureOffset(27,26); body.addBox(-8,0,-9,4,4,12); body.setTextureOffset(27,26); body.addBox(4,0,-7,4,4,12);} var ghost_modelRenderType = Renderer.createHumanoidRenderer(); addghost_modelRenderType(ghost_modelRenderType);